@article{Kobele2010,
  title = {Inverse Linking via Function Composition},
  author = {Kobele, Gregory M.},
  date = {2010-06-01},
  journaltitle = {Natural Language Semantics},
  shortjournal = {Nat Lang Semantics},
  volume = {18},
  number = {2},
  pages = {183--196},
  issn = {1572-865X},
  doi = {10.1007/s11050-009-9053-7},
  url = {https://doi.org/10.1007/s11050-009-9053-7},
  urldate = {2020-02-17},
  abstract = {The phenomenon of inverse linking, where a noun phrase embedded within another behaves with respect to binding as though it were structurally independent, has proven challenging for theories of the syntax–semantics interface. In this paper I show that, using an LF-movement style approach to the syntax–semantics interface, we can derive all and only the appropriate meanings for such constructions using no semantic operations other than function application and composition. The solution relies neither on a proliferation of lexical ambiguity nor on abandoning the idea that pronouns denote variables, but rather on a straightforward (and standard) reification of assignment functions, which allows us to define abstraction operators within our models.},
  langid = {english}
}

@article{Kobele2018,
  title = {The {{Cooper Storage Idiom}}},
  author = {Kobele, Gregory M.},
  date = {2018-06-01},
  journaltitle = {Journal of Logic, Language and Information},
  shortjournal = {J. of Logic, Lang. and Inf.},
  volume = {27},
  number = {2},
  pages = {95--131},
  issn = {0925-8531},
  doi = {10.1007/s10849-017-9263-1},
  url = {https://doi.org/10.1007/s10849-017-9263-1},
  urldate = {2021-04-21},
  abstract = {Cooper storage is a widespread technique for associating sentences with their meanings, used (sometimes implicitly) in diverse linguistic and computational linguistic traditions. This paper encodes the data structures and operations of cooper storage in the simply typed linear \$\$\textbackslash lambda \$\$ź-calculus, revealing the rich categorical structure of a graded applicative functor. In the case of finite cooper storage, which corresponds to ideas in current transformational approaches to syntax, the semantic interpretation function can be given as a linear homomorphism acting on a regular set of trees, and thus generation can be done in polynomial time.},
  keywords = {Applicative functor,Compositionality,Cooper storage,Lambda calculus}
}

