@article{Geurts2005,
  title = {Entertaining {{Alternatives}}: {{Disjunctions}} as {{Modals}}},
  shorttitle = {Entertaining {{Alternatives}}},
  author = {Geurts, Bart},
  date = {2005-12-01},
  journaltitle = {Natural Language Semantics},
  shortjournal = {Nat Lang Seman},
  volume = {13},
