@inproceedings{LaiEtAl2020,
  title = {A {{Continuation Semantics}} for {{Abstract Meaning Representation}}},
  booktitle = {Proceedings of the {{Second International Workshop}} on {{Designing Meaning Representations}}},
  author = {Lai, Kenneth and Donatelli, Lucia and Pustejovsky, James},
  date = {2020-12},
  pages = {1--12},
  publisher = {Association for Computational Linguistics},
  location = {Barcelona Spain (online)},
  url = {https://aclanthology.org/2020.dmr-1.1},
  urldate = {2021-07-27},
  abstract = {Abstract Meaning Representation (AMR) is a simple, expressive semantic framework whose emphasis on predicate-argument structure is effective for many tasks. Nevertheless, AMR lacks a systematic treatment of projection phenomena, making its translation into logical form problematic. We present a translation function from AMR to first order logic using continuation semantics, which allows us to capture the semantic context of an expression in the form of an argument. This is a natural extension of AMR's original design principles, allowing us to easily model basic projection phenomena such as quantification and negation as well as complex phenomena such as bound variables and donkey anaphora.},
  eventtitle = {{{COLING-DMR}} 2020},
  keywords = {Abstract Meaning Representation}
}

