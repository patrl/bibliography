@article{Wu2022,
  title = {Syntax of Either in Either… Or… Sentences},
  author = {Wu, Danfeng},
  date = {2022-08-01},
  journaltitle = {Natural Language \& Linguistic Theory},
  shortjournal = {Nat Lang Linguist Theory},
  volume = {40},
  number = {3},
  pages = {933--977},
  issn = {1573-0859},
  doi = {10.1007/s11049-021-09523-0},
  url = {https://doi.org/10.1007/s11049-021-09523-0},
  urldate = {2022-08-15},
  abstract = {I propose an analysis of either in either… or … sentences that relates to a broader generalization about the syntax of all focus-sensitive operators. I argue that either originates inside the disjunction phrase, c-commanding the leftmost contrastive focus. Then it is internally merged as the sister of the disjunction phrase. Either copy of either may be pronounced. When either appears higher than the sister of the disjunction phrase, bare argument ellipsis has occurred in the second disjunct. This analysis of either is consonant with the generalization that all focus-sensitive operators occupy two positions in a sentence (e.g. Cable 2007; Hirsch 2017; and Quek and Hirsch 2017). If this analysis of either is correct, then it not only indicates that either is a focus-sensitive operator, but also adds another data point to this universal generalization.},
  langid = {english},
  keywords = {Bare argument ellipsis,Disjunction,Either,Ellipsis,Focus,Focus-sensitive operators,Islands,Movement,Or,Scope,Stripping,Syntax}
}

