@article{RothschildYalcin2016,
  title = {Three Notions of Dynamicness in Language},
  author = {Rothschild, Daniel and Yalcin, Seth},
  date = {2016-08-01},
  journaltitle = {Linguistics and Philosophy},
  shortjournal = {Linguist and Philos},
  volume = {39},
  pages = {333--355},
  issn = {1573-0549},
  doi = {10.1007/s10988-016-9188-1},
  url = {https://doi.org/10.1007/s10988-016-9188-1},
  urldate = {2020-08-02},
  abstract = {We distinguish three ways that a theory of linguistic meaning and communication might be considered dynamic in character. We provide some examples of systems which are dynamic in some of these senses but not others. We suggest that separating these notions can help to clarify what is at issue in particular debates about dynamic versus static approaches within natural language semantics and pragmatics.},
